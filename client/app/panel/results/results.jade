.container-fluid
  .row(style="padding:10px;")
    paper-shadow.col-sm-12
      h3 Recent Match Results
      table.table.table-hover.table-condensed(style="display:inline-block;overflow-x:auto")
        thead
          tr
        tbody
          tr(ng-repeat="profile in profiles | orderBy:['-profile.rating', 'profile.name']",ng-click="goToProfile(profile)")
